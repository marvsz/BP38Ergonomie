QMainWindow {background-color: #efeff4;}

QWidget {background-color: #efeff4}
QWidget#detailedListItemWidget {border:2px solid #15c142; border-radius: 10px;}
AbstractPopUpWidget {border:2px solid #15c142; border-radius: 10px;}
QWidget#popUpBackground {background-color: rgba(150, 150, 150, 180);}
QWidget#notifiMessageBackground {background: transparent;}
Separator {background-color: #c8c7cc; margin: 5px 5px 5px 5px;}

QLabel {font: 100 18px "Serif"; color: #8e8e93;}
QLabel#lblHeader {font: 100 18px "Serif"; color: #15c142;}
QLabel#lblCaption {font: 100 17px "Serif"; color: #15c142;}
QLabel#lblValue {font: 100 16px "Serif"; color: #8e8e93;}

QLineEdit {font: 100 18px "Serif";border: 2px solid #15c142; border-radius: 10px; background: #f5f5f5; color: #8e8e93; padding: 4px;}
TextEdit {font: 100 18px "Serif";border: 2px solid #15c142; border-radius: 10px; background: #f5f5f5; color: #8e8e93; padding: 4px;}

QSlider::handle:horizontal {image: url(:/green/icons/Buttons/green/handle.png); width: 40px; height: 40px; margin: -20px 0;}
