NotificationMessage {font: 100 18px "Serif"; color: #f5f5f5; border: 2px solid rgba(150, 150, 150, 180); border-radius: 12px; background-color: rgba(150, 150, 150, 180); padding: 4px;}
NotificationMessage:pressed {font: 100 18px "Serif"; color: #f5f5f5; border: 2px solid rgba(150, 150, 150, 180); border-radius: 12px; background-color: rgba(0, 122, 255, 180); padding: 4px;}

QScrollArea {background-color: #efeff4; margin: 0 0; border: none;}
QScrollArea#saBordered {border: 1px solid #c8c7cc; border-radius: 2px;}
QScrollBar:vertical {border: none solid #15c142; margin: 0px 0px 0px 0px; background: #efeff4;}
QScrollBar::handle:vertical {background: #15c142; min-width: 60px; border-radius: 5px;}
QScrollBar::sub-line:vertical, QScrollBar::add-line:vertical  {border: none solid #15c142; background: #efeff4;}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {width: 0px; height: 0px;}
QScrollBar:horizontal {border: none solid #15c142; margin: 0px 0px 0px 0px; background: #efeff4;}
QScrollBar::handle:horizontal {background: #15c142; min-width: 60px; border-radius: 5px;}
QScrollBar::sub-line:horizontal, QScrollBar::add-line:horizontal  {border: none solid #15c142; background: #efeff4;}

QGroupBox {border:2px solid #15c142; background-color: #efeff4; border-radius: 10px;}

QComboBox {font: 100 18px "Serif"; color: #15c142; border: 2px solid #15c142; border-radius: 10px; background-color: #f5f4f4; padding: 4px;}
QComboBox::drop-down {background-color: #15c142;}
QComboBox::down-arrow {image: url(:/green/icons/Buttons/green/down.png); width: 15px; height: 15px;}
QComboBox::down-arrow:on {image: url(:/green/icons/Buttons/green/up.png); width: 15px; height: 15px;}
